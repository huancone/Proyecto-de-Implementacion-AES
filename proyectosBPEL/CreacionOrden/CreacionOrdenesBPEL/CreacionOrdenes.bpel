                              <from variable="inputVariable" part="payload"
                                    query="/client:process/client:itemEspectaculo/client:cantEspectaculo"/>
                              <to variable="InvResDorado_In" part="parameters"
                                  query="/ns1:ValidarDisponibilidadReservas/itemEspectaculo/cantidad"/>
                            </copy>
                            <copy>
                              <from variable="inputVariable" part="payload"
                                    query="/client:process/client:itemTransporte/client:idTransporte"/>
                              <to variable="InvResDorado_In" part="parameters"
                                  query="/ns1:ValidarDisponibilidadReservas/itemTransporte/idItem"/>
                            </copy>
                            <copy>
                              <from variable="inputVariable" part="payload"
                                    query="/client:process/client:itemTransporte/client:nombreTransporte"/>
                              <to variable="InvResDorado_In" part="parameters"
                                  query="/ns1:ValidarDisponibilidadReservas/itemTransporte/nombreItem"/>
                            </copy>
                            <copy>
                              <from variable="inputVariable" part="payload"
                                    query="/client:process/client:itemTransporte/client:precioTransporte"/>
                              <to variable="InvResDorado_In" part="parameters"
                                  query="/ns1:ValidarDisponibilidadReservas/itemTransporte/precioItem"/>
                            </copy>
                            <copy>
                              <from variable="inputVariable" part="payload"
                                    query="/client:process/client:itemTransporte/client:cantTransporte"/>
                              <to variable="InvResDorado_In" part="parameters"
