                                    query="/client:process/client:itemEspectaculo/client:idEspectaculo"/>
                              <to variable="InvResDorado_In" part="parameters"
                                  query="/ns1:ValidarDisponibilidadReservas/itemEspectaculo/idItem"/>
                            </copy>
