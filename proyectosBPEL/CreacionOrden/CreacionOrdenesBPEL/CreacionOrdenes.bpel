            <bpelx:property name="userLabel">
              <![CDATA[HayDisponibilidaReservas]]>
            </bpelx:property>
          </bpelx:general>
        </bpelx:annotation>
        <sequence name="Sequence1">
          <switch name="Switch2">
            <case condition="bpws:getVariableData('inputVariable','payload','/client:process/client:clienteStatus') = &quot;PLATINO&quot;">
              <bpelx:annotation>
                <bpelx:general>
                  <bpelx:property name="userLabel">
                    <![CDATA[EsPlatino]]>
                  </bpelx:property>
                </bpelx:general>
              </bpelx:annotation>
              <sequence name="Sequence2">
                <assign name="AsigReserva">
                  <copy>
                    <from variable="inputVariable" part="payload"
                          query="/client:process/client:itemEspectaculo/client:idEspectaculo"/>
                    <to variable="InvReservarProducto_In" part="parameters"
                        query="/ns1:ReservarProductos/itemEspectaculo/idItem"/>
                  </copy>
                  <copy>
                    <from variable="inputVariable" part="payload"
                          query="/client:process/client:itemEspectaculo/client:nombreEspectaculo"/>
                    <to variable="InvReservarProducto_In" part="parameters"
