                            </copy>
                            <copy>
                              <from variable="inputVariable" part="payload"
                                    query="/client:process/client:itemHospedaje/client:nombreHospedaje"/>
                              <to variable="InvResDorado_In" part="parameters"
                                  query="/ns1:ValidarDisponibilidadReservas/itemHospedaje/nombreItem"/>
                            </copy>
